{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "kanshi",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "profile"
      }
    },
    "profile": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "profile"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "output"
              },
              {
                "type": "SYMBOL",
                "name": "comment"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "output": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "output"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "mode",
                      "content": {
                        "type": "SYMBOL",
                        "name": "mode"
                      }
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "position",
                      "content": {
                        "type": "SYMBOL",
                        "name": "position"
                      }
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "FIELD",
                      "name": "scale",
                      "content": {
                        "type": "SYMBOL",
                        "name": "scale"
                      }
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "STRING",
              "value": "disable"
            }
          ]
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]*"
          }
        ]
      }
    },
    "mode": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "mode"
        },
        {
          "type": "PATTERN",
          "value": "\\d+x\\d+@\\d+(\\.\\d+)?(Hz)?"
        }
      ]
    },
    "position": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "position"
        },
        {
          "type": "PATTERN",
          "value": "\\d+,\\d+"
        }
      ]
    },
    "scale": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "scale"
        },
        {
          "type": "PATTERN",
          "value": "[0-9]+(\\.[0-9]+)?"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "PATTERN",
          "value": "[^\"]*"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
